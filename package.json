{
  "name": "mo",
  "version": "0.1.0",
  "description": "CircleCI client for mobile web",
  "scripts": {
    "test": "jest",
    "clean:build": "if [ -d build ]; then rm -rf build; fi",
    "predev": "npm run clean:build",
    "dev": "node server",
    "prebuild": "npm run clean:build",
    "build": "next build",
    "deploy": "now --public -e NODE_ENV=production -n moci --no-clipboard",
    "alias": "now alias --no-clipboard",
    "start": "NODE_ENV=production node server"
  },
