{
  "name": "mo",
  "version": "2.0.0",
  "description": "CircleCI client for mobile web",
  "private": true,
  "scripts": {
    "predev": "npm run clean:build",
    "dev": "NODE_ENV=development ts-node-dev --respawn --transpileOnly src/server/index.ts",
    "build": "nuxt-ts build",
    "start": "NODE_ENV=production node src/server/index.js",
    "generate": "nuxt-ts generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "jest",
    "clean:build": "if [ -d build ]; then rm -rf build; fi",
    "prebuild": "npm run clean:build",
    "deployment:pop": "node ./scripts/deployment-pop",
    "deployment:delete": "node ./scripts/deployment-delete"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sugarshin/mo.git"
  },
  "author": "sugarshin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sugarshin/mo/issues"
  },
  "homepage": "https://github.com/sugarshin/mo#readme",
  "dependencies": {
    "@nuxtjs/axios": "5.3.6",
    "@nuxtjs/proxy": "1.3.2",
    "@nuxtjs/pwa": "3.0.0-beta.6",
    "@types/express": "4.16.1",
    "@types/node": "10.12.26",
    "@types/webpack": "4.4.24",
    "bootstrap": "4.3.1",
    "bootstrap-vue": "2.0.0-rc.12",
    "consola": "2.4.1",
    "express": "4.16.4",
    "nuxt": "2.4.3",
    "nuxt-ts": "2.4.3"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "0.0.1",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.1.0",
    "eslint": "5.13.0",
    "eslint-config-prettier": "4.0.0",
    "eslint-config-standard": "12.0.0",
    "eslint-loader": "2.1.2",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jest": "22.3.0",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-standard": "4.0.0",
    "eslint-plugin-vue": "5.2.1",
    "jest": "24.1.0",
    "prettier": "1.16.4",
    "ts-node-dev": "1.0.0-pre.32",
    "vue-jest": "3.0.3"
  }
}
